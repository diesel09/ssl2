DROPBEAR_EXTRA_ARGS="-p $ptdo "
DROPBEAR_RECEIVE_WINDOW=65536
EOF
if cat /etc/shells|grep "/bin/false" > /dev/null; then
clear
else
echo -e "/bin/false" >> /etc/shells
fi
service ssh restart > /dev/null
