screen -dmS v2ser v2raypro -config /etc/v2ray/config.json >/dev/null 2>&1
echo -e "${lor2} SERVICE STARTED ${lor7}"
fi
else
echo -e "${lor1} V2RAY NOT INSTALLED ${lor7}"
fi
fi
espe
